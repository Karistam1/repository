@article{hodgeMultiscalar3DTemporal2024,
  title = {Multiscalar {{3D}} Temporal Structural Characterisation of {{Smøla}} Island, Mid-{{Norwegian}} Passive Margin: An Analogue for Unravelling the Tectonic History of Offshore Basement Highs},
  shorttitle = {Multiscalar {{3D}} Temporal Structural Characterisation of {{Smøla}} Island, Mid-{{Norwegian}} Passive Margin},
  author = {Hodge, Matthew S. and Venvik, Guri and Knies, Jochen and family=Lelij, given=Roelant, prefix=van der, useprefix=true and Schönenberger, Jasmin and Nordgulen, Øystein and Brönner, Marco and Nasuti, Aziz and Viola, Giulio},
  date = {2024-05-13},
  journaltitle = {Solid Earth},
  volume = {15},
  number = {5},
  pages = {589--615},
  publisher = {Copernicus GmbH},
  issn = {1869-9510},
  doi = {10.5194/se-15-589-2024},
  url = {https://se.copernicus.org/articles/15/589/2024/},
  urldate = {2024-12-20},
  abstract = {Smøla island, situated within the mid-Norwegian passive margin, contains crystalline-basement-hosted intricate fracture and fault arrays formed during a polyphase brittle tectonic evolution. Its detailed study may strengthen correlation attempts between the well-exposed onshore domain and the inaccessible offshore domain, further the understanding of the passive margin evolution, and provide useful constraints on petrophysical properties of fractured basement blocks. A combination of geophysical and remote sensing lineament analysis, field mapping, high-resolution drill hole logging, 3D modelling, petrographic and microstructural studies, and fault gouge K–Ar geochronology made it possible to define five deformation episodes (D1 to D5). These episodes occurred between the post-Caledonian evolution of the regional-scale Møre–Trøndelag Fault Complex (MTFC) and the Late Cretaceous and younger crustal extension preceding the final stages of Greenland–Norway break-up. Each reconstructed deformation stage is associated with different structural features, fault and fracture geometries, and kinematic patterns. Synkinematic mineralisations evolved progressively from epidote–prehnite, sericite–chlorite–calcite, chlorite–hematite, hematite–zeolite–calcite, to quartz–calcite. K–Ar geochronology constrains brittle deformation to discrete localisation events spanning from the Carboniferous to the Late Cretaceous. Multiscalar geometrical modelling at scales of 100, 10, and 1 m helps constrain the extent and size of the deformation zones of each deformation episode, with D2 structures exhibiting the greatest strike continuity and D1 features the most localised. Overall, the approach highlighted here is of great utility for unravelling complex brittle tectonic histories within basement volumes. It is also a prerequisite to constrain the dynamic evolution of the petrophysical properties of basement blocks.},
  langid = {english}
}

@article{imberFrictionalviscousFlowSeismicity2008,
  title = {Frictional-Viscous Flow, Seismicity and the Geology of Weak Faults: A Review and Future Directions},
  shorttitle = {Frictional-Viscous Flow, Seismicity and the Geology of Weak Faults},
  author = {Imber, J. and Holdsworth, R. E. and Smith, S. A. F. and Jefferies, S. P. and Collettini, C.},
  date = {2008-01},
  journaltitle = {Geological Society, London, Special Publications},
  shortjournal = {SP},
  volume = {299},
  number = {1},
  pages = {151--173},
  issn = {0305-8719, 2041-4927},
  doi = {10.1144/SP299.10},
  url = {https://www.lyellcollection.org/doi/10.1144/SP299.10},
  urldate = {2024-09-16},
  abstract = {Abstract             Previously hypothesized fault weakening mechanisms include faults lined by low-friction clay gouges, elevated pore pressures within fault cores and/or the operation of dynamic weakening during seismic slip. Geological studies to support dynamic weakening are still in their infancy and there is little geological evidence for the widespread occurrence of low-friction gouges. The cores of some ancient faults exhumed from {$<$}5 km depth contain sheared syntectonic mineral veins. This observation is consistent with elevated pore pressures, but the implications for long-term fault weakening are unclear. Experimental data and microphysical modelling suggest that frictional–viscous flow within phyllosilicate-rich fault rocks (phyllonites, some foliated cataclasites) can cause sufficient weakening of crustal faults to satisfy published heat flow constraints. These predictions are consistent with the common occurrence of phyllonite in the cores of large-displacement faults exhumed from {$>$}5 km depth. Comparison with seismological data suggests that some faults with phyllonitic cores are likely to generate large earthquakes. Future studies should establish the geological evidence for seismic slip within phyllonitic fault cores and quantify the partitioning between seismic slip and frictional–viscous flow. Further geological observations are also required to test the hypothesized mechanisms by which earthquakes can nucleate and propagate along phyllosilicate-rich faults.},
  langid = {english}
}

@thesis{johansenDetachmentFaultingFaultcontrolled2022,
  type = {mathesis},
  title = {Detachment Faulting and Fault-Controlled Sub-Basin Formation at the Northern Margin of the {{Devonian Hornelen}} Basin, {{Western Norway}}},
  author = {Johansen, Sindre Ekrene},
  date = {2022},
  institution = {NTNU},
  url = {https://ntnuopen.ntnu.no/ntnu-xmlui/handle/11250/3016409},
  urldate = {2024-11-25},
  abstract = {Devonbassengene i Vest-Norge ble dannet i forbindelse med storskala normalforkastning i ekstensjonale detachment-soner. Disse bassengene var utsatt for syn-sedimentær ekstensjonsparallell folding, i tillegg til ekstensjonsrotering av de sedimentære lagene. Hornelen-bassenget består hovedsakelig av fluviale og lakustrine sandsteiner, og vifte-konglomerater langs bassengmarginene, som til sammen utgjør de tre «lithofacies-assosiasjonene». Den lavvinklede detachment-forkastningssonen som skiller de devonske sedimentene fra de kaledonske skyvedekkene og de proterozoiske grunnfjellsbergartene eksponerer forkastningsbergarter som representerer ulike deformasjonsstadier.  Denne masteroppgaven fokuserer hovedsakelig på de tektoniske kontaktene innad i bassenget i den nordlige delen, i tillegg til utviklingen av forkastningssone-produktene. Feltarbeid, optisk mikroskopering og «remote sensing» la grunnlaget som denne oppgaven er basert på.  Ved Svartevatnet, en rotert normalforkastning som sidestiller vifte-konglomerater i liggblokken med en blanding av de tre «lithofacies assosiasjonene» i hengblokken. Den anslåtte innsynkningen av sub-bassenget i hengblokken resulterte i et sedimentasjonsmiljø kontrollert av Svartevatnet-forkastningen. Innsjø-forflytning, elveløp-defleksjon og re-sedimentasjon via den opphøyde liggblokken er sannsynlige konsekvenser.  Kvantitativ rotasjon av forkastningsplanet og forkastningslineasjonene indikerer en nesten ekstensjons-vinkelrett, brattfallende, skrå «dip-slip» forkastning da den var aktiv. Den er anslått å være syn-sedimentær.  Syn-sedimentære forkastnings-relatert folding i form av, normale «drag-folder» og halvgraben «rollover-folder» var observert rundt forkastningsplanet. Basseng-skala lagningsrotasjon ble observert rett ved forkastningen, noe som tilsier at Svartevatnet-forkastningen spilte en viktig rolle under utviklingen av Hornelen-bassenget.  Detachment-forkastningssonen som slår gjennom Marøyna, ei øy rett vest for den nordlige delen av fastlandet, og forkastningsbergarter er derfor godt eksponert langs kystlinjen. Hvilke bergarter som kutter hverandre, samt klastinnholdet til bergartene dokumenterer en gradvis ekshumering fra den duktile delen av jordskorpa, gjennom den duktil-sprø-overgangssonen, og inn i den sprø delen av skorpa.  Betydelig laterale variasjoner på regional-, blotning- og mikroskala indikerer ekstrem tøyningslokalisering lang detachment-sonen. Faktorer som mineralogi, kornstørrelsesreduksjonsprosesser og fluidinjiseringer var sannsynligvis viktige bidragsytere til denne tøyningslokaliseringen.},
  langid = {english},
